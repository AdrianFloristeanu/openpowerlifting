{{#*inline "topbar-hack"}}
  <div class="topbar">
{{/inline}}
{{#*inline "topbar-hack-end"}}
  </div>
{{/inline}}

{{#*inline "controls"}}
  <div id="controls">
    <div id="controls-left">
      <span id="lifter">Meet Results for {{lifter.name}}</span>
      <select id="weighttype">
        <option value="lb">lbs</option>
        <option value="kg">kg</option>
      </select>
    </div>
  </div>
{{/inline}}

{{#*inline "body"}}
  <div class="content">
    {{! Meet results table. }}
    <table>
      <thead>
        <th>Place</th>
        <th>Fed</th>
        <th>Date</th>
        <th>Location</th>
        <th>Meet Name</th>
        <th>Division</th>
        <th>Sex</th>
        <th>Equip</th>
        <th>Class</th>
        <th>Weight</th>
        <th>Squat</th>
        <th>Bench</th>
        <th>Deadlift</th>
        <th>Total</th>
        <th>Wilks</th>
      </thead>
      {{#each meet_results}}
        <tr>
          <td>{{this.place}}</td>
          <td>{{this.federation}}</td>
          <td>{{this.date}}</td>
          <td>{{this.country}}{{#if this.state}}-{{this.state}}{{/if}}</td>
          <td><a href="/m/{{this.meet_path}}">{{this.meet_name}}</a></td>
          <td>{{this.division}}</td>
          <td>{{this.sex}}</td>
          <td>{{this.equipment}}</td>
          <td>{{this.weightclasskg}}</td>
          <td>{{this.bodyweightkg}}</td>
          <td>{{this.squatkg}}</td>
          <td>{{this.benchkg}}</td>
          <td>{{this.deadliftkg}}</td>
          <td>{{this.totalkg}}</td>
          <td>{{this.wilks}}</td>
        </tr>
      {{/each}}
    </table>
  </div>
{{/inline}}

{{~> desktop-base}}
